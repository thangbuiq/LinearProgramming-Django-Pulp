{
    "version": 2,
    "builds": [
        {
            "src": "lp_app/**/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/.*",
            "dest": "/lp_app/wsgi.py"
        }
    ]
}